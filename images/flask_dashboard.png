<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600" width="800" height="600">
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f5f5"/>
  
  <!-- Dashboard Header -->
  <rect x="50" y="50" width="700" height="80" rx="10" fill="#4285f4"/>
  <text x="100" y="100" font-family="Arial" font-size="30" fill="white">Flask Dashboard</text>
  
  <!-- Navigation Menu -->
  <rect x="50" y="150" width="200" height="400" rx="10" fill="#e0e0e0"/>
  <rect x="70" y="180" width="160" height="40" rx="5" fill="#4285f4"/>
  <text x="100" y="205" font-family="Arial" font-size="16" fill="white">Dashboard</text>
  <rect x="70" y="240" width="160" height="40" rx="5" fill="white"/>
  <text x="100" y="265" font-family="Arial" font-size="16" fill="#333">Analytics</text>
  <rect x="70" y="300" width="160" height="40" rx="5" fill="white"/>
  <text x="100" y="325" font-family="Arial" font-size="16" fill="#333">Reports</text>
  <rect x="70" y="360" width="160" height="40" rx="5" fill="white"/>
  <text x="100" y="385" font-family="Arial" font-size="16" fill="#333">Settings</text>
  
  <!-- Main Content Area -->
  <rect x="270" y="150" width="480" height="400" rx="10" fill="white"/>
  
  <!-- Chart 1: Bar Chart -->
  <rect x="290" y="170" width="220" height="180" rx="5" fill="#f8f9fa"/>
  <rect x="310" y="270" width="20" height="60" fill="#4285f4"/>
  <rect x="340" y="240" width="20" height="90" fill="#4285f4"/>
  <rect x="370" y="220" width="20" height="110" fill="#4285f4"/>
  <rect x="400" y="200" width="20" height="130" fill="#4285f4"/>
  <rect x="430" y="250" width="20" height="80" fill="#4285f4"/>
  <rect x="460" y="230" width="20" height="100" fill="#4285f4"/>
  <text x="310" y="200" font-family="Arial" font-size="14" fill="#333">Weekly Data</text>
  
  <!-- Chart 2: Line Chart -->
  <rect x="530" y="170" width="200" height="180" rx="5" fill="#f8f9fa"/>
  <polyline points="550,300 570,280 590,290 610,260 630,270 650,240 670,250 690,220" stroke="#4285f4" stroke-width="3" fill="none"/>
  <text x="550" y="200" font-family="Arial" font-size="14" fill="#333">Trends</text>
  
  <!-- Data Cards -->
  <rect x="290" y="370" width="140" height="100" rx="5" fill="#e8f0fe"/>
  <text x="310" y="400" font-family="Arial" font-size="14" fill="#333">Total Users</text>
  <text x="310" y="430" font-family="Arial" font-size="24" font-weight="bold" fill="#4285f4">1,234</text>
  
  <rect x="450" y="370" width="140" height="100" rx="5" fill="#e8f0fe"/>
  <text x="470" y="400" font-family="Arial" font-size="14" fill="#333">Active Sessions</text>
  <text x="470" y="430" font-family="Arial" font-size="24" font-weight="bold" fill="#4285f4">567</text>
  
  <rect x="610" y="370" width="140" height="100" rx="5" fill="#e8f0fe"/>
  <text x="630" y="400" font-family="Arial" font-size="14" fill="#333">Conversion Rate</text>
  <text x="630" y="430" font-family="Arial" font-size="24" font-weight="bold" fill="#4285f4">8.5%</text>
  
  <!-- Flask Logo -->
  <g transform="translate(680, 90) scale(0.4)">
    <path d="M20,50 C20,30 40,20 60,20 C80,20 100,30 100,50 L100,100 C100,120 80,130 60,130 C40,130 20,120 20,100 Z" fill="none" stroke="white" stroke-width="5"/>
    <circle cx="60" cy="40" r="5" fill="white"/>
    <circle cx="60" cy="110" r="5" fill="white"/>
  </g>
</svg>